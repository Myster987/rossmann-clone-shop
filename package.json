{
	"name": "shop-fix",
	"version": "0.0.1",
	"private": true,
	"scripts": {
		"db:generate": "drizzle-kit generate --config=./src/lib/db/drizzle.config.ts",
		"db:migrate": "node ./src/lib/db/migrate.ts",
		"db:studio": "drizzle-kit studio --config=./src/lib/db/drizzle.config.ts",
		"db:push": "drizzle-kit push --config=./src/lib/db/drizzle.config.ts",
		"db:local": "turso dev -f ../db/local.db",
		"dev-admin": "cd ../ && cd rossmann-clone-admin && pnpm run dev",
		"dev": "vite dev",
		"build": "vite build",
		"preview": "vite preview",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --check . && eslint .",
		"format": "prettier --write ."
	},
	"devDependencies": {
		"@sveltejs/adapter-auto": "^3.0.0",
		"@sveltejs/kit": "^2.0.0",
		"@sveltejs/vite-plugin-svelte": "^3.0.0",
		"@types/eslint": "^8.56.0",
		"@typescript-eslint/eslint-plugin": "^7.0.0",
		"@typescript-eslint/parser": "^7.0.0",
		"autoprefixer": "^10.4.16",
		"drizzle-kit": "^0.21.2",
		"eslint": "^8.56.0",
		"eslint-config-prettier": "^9.1.0",
		"eslint-plugin-svelte": "^2.35.1",
		"postcss": "^8.4.33",
		"prettier": "^3.1.1",
		"prettier-plugin-svelte": "^3.1.2",
		"prettier-plugin-tailwindcss": "^0.5.1",
		"svelte": "^4.2.7",
		"svelte-check": "^3.6.0",
		"tailwindcss": "^3.4.1",
		"tslib": "^2.4.1",
		"typescript": "^5.0.0",
		"vite": "^5.0.3"
	},
	"type": "module",
	"dependencies": {
		"@hono/zod-validator": "^0.2.1",
		"@libsql/client": "^0.15.10",
		"@lucia-auth/adapter-drizzle": "^1.0.7",
		"bits-ui": "^0.21.9",
		"clsx": "^2.1.1",
		"cmdk-sv": "^0.0.17",
		"drizzle-orm": "^0.30.10",
		"embla-carousel-svelte": "^8.1.1",
		"formsnap": "^1.0.0",
		"hono": "^4.3.7",
		"lucia": "^3.2.0",
		"lucide-svelte": "^0.378.0",
		"mode-watcher": "^0.3.0",
		"oslo": "^1.2.0",
		"stripe": "^15.8.0",
		"svelte-sonner": "^0.3.24",
		"sveltekit-superforms": "^2.13.1",
		"tailwind-merge": "^2.3.0",
		"tailwind-variants": "^0.2.1",
		"zod": "^3.23.8"
	}
}
